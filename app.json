{
  "expo": {
    "name": "Medu Chat",
    "slug": "Medu Chat",
    "assetBundlePatterns": ["assets/*"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-prod.png",
    "scheme": "medullmoffline",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "llm",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-prod.png",
        "backgroundColor": "#0f1115"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jorge_assaf01.medullmoffline"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-prod.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f1115"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38eff257-05bb-4021-a5c1-911f1292268c"
      }
    }
  }
}
